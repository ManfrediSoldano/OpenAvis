{
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/api/*",
            "/*.{js,css,json,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot}"
        ]
    },
    "platform": {
        "apiRuntime": "node:18"
    }
}