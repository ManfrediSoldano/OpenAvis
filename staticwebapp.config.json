{
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/api/*",
            "/*.{js,css,json,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot}"
        ]
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https:; img-src 'self' data: https:; font-src 'self' data: https:;"
    }
}